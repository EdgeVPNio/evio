{
  "CFx": {
    "Overlays": [ "TMPLOVL1" ]
  },
  "Logger": {
    "LogLevel": "WARNING",
    "Directory": "/var/log/evio/"
  },
  "Signal": {
    "Overlays": {
      "TMPLOVL1": {
        "HostAddress": "***.***.***.***",
        "Port": "5222",
        "Username": "***@***",
        "Password": "***",
        "AuthenticationMethod": "PASSWORD"
      }
    }
  },
  "Topology": {
    "Overlays": {
      "TMPLOVL1": {
        "MaxSuccessors": 2,
        "MaxOnDemandEdges": 3,
        "Role": "Switch"
      }
    }
  },
  "LinkManager": {
    "Stun": [ "stun.l.google.com:19302", "stun1.l.google.com:19302" ],
    "Turn": [{
      "Address": "***.***.***:3478",
      "User": "***",
      "Password": "***"
     }],
    "Overlays": {
      "TMPLOVL1": {
        "Type": "TUNNEL",
        "TapName": "tnl-"
      }
    }
  },
  "BridgeController": {
    "BoundedFlood": {
        "OverlayId": "TMPLOVL1",
        "LogDir": "/var/log/evio/",
        "LogFilename": "bf.log",
        "LogLevel": "WARNING",
        "BridgeName": "evio",
        "DemandThreshold": "100M",
        "FlowIdleTimeout": 60,
        "FlowHardTimeout": 60,
        "MulticastBroadcastInterval": 60,
        "MaxBytes": 10000000,
        "BackupCount": 2,
        "ProxyListenAddress": "",
        "ProxyListenPort": 5802,
        "MonitorInterval": 60,
        "MaxOnDemandEdges": 3
    },
    "Overlays": {
        "TMPLOVL1": {
            "NetDevice": {
                "AutoDelete": true,
                "Type": "OVS",
                "SwitchProtocol": "BF",
                "NamePrefix": "evio",
                "IP4": "10.10.10.*",
                "PrefixLen": 24,
                "MTU": 1410,
                "AppBridge": {
                   "AutoDelete": true,
                    "Type": "OVS",
                    "NamePrefix": "appbr",
                    "IP4": "10.10.10.*",
                    "PrefixLen": 24,
                    "MTU": 1410
                }
            },
            "SDNController": {
                "ConnectionType": "tcp",
                "HostName": "127.0.0.1",
                "Port": "6633"
            }
        }
    }
  },
  "UsageReport": {
    "TimerInterval": 86400,
    "WebService": "https://qdscz6pg37.execute-api.us-west-2.amazonaws.com/default/EvioUsageReport"
  }
}
