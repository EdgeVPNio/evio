        "MonitorInterval": 60,
        "MaxOnDemandEdges": 0
    },
    "Overlays": {
        "A0FB389": {
            "NetDevice": {
                "AutoDelete": true,
                "Type": "OVS",
                "SwitchProtocol": "BF",
                "NamePrefix": "evio",
                "IP4": "10.10.10.*",
                "PrefixLen": 24,
                "MTU": 1410,
                "GuestBridge": {
                   "AutoDelete": true,
                    "Type": "OVS",
                    "NamePrefix": "appbr",
                    "IP4": "10.10.10.*",
                    "PrefixLen": 24,
                    "MTU": 1410
                }
            },
            "SDNController": {
                "ConnectionType": "tcp",
                "HostName": "127.0.0.1",
                "Port": "6633"
            }
        }
    }
  },
  "UsageReport": {
    "Enabled": true,
    "TimerInterval": 86400,
    "WebService": "https://2zkan9566m.execute-api.us-west-2.amazonaws.com/default/PythonEndpointMicroservice"
  }
