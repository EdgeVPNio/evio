{
  "CFx": {
    "Model": "Default",
    "Overlays": [ "A0FB389" ]
  },
  "Logger": {
    "LogLevel": "WARNING",
    "Device": "File",
    "Directory": "/var/log/edge-vpnio/",
    "CtrlLogFileName": "ctrl.log",
    "TincanLogFileName": "tincan_log",
    "MaxFileSize": 10000000,
    "MaxArchives": 2
  },
  "Signal": {
    "Enabled": true,
    "Overlays": {
      "A0FB389": {
        "HostAddress": "***.***.***.***",
        "Port": "5222",
        "Username": "***@***",
        "Password": "***",
        "AuthenticationMethod": "PASSWORD"
      }
    }
  },
  "Topology": {
    "PeerDiscoveryCoalesce": 1,
    "Overlays": {
      "A0FB389": {
        "Name": "ApplicationOverlay",
